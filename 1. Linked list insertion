class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

class LinkedList:
    def __init__(self):
        self.head = None

    def insert_begin(self, data):
        n = Node(data)
        n.next = self.head
        self.head = n

    def insert_end(self, data):
        n = Node(data)
        if not self.head:
            self.head = n
            return
        t = self.head
        while t.next:
            t = t.next
        t.next = n

    def insert_after(self, key, data):
        t = self.head
        while t and t.data != key:
            t = t.next
        if not t: return
        n = Node(data)
        n.next = t.next
        t.next = n

    def display(self):
        t = self.head
        while t:
            print(t.data, end=" -> ")
            t = t.next
        print("None")

# Example
ll = LinkedList()
ll.insert_begin(10)
ll.insert_end(20)
ll.insert_end(30)
ll.insert_after(20, 25)
ll.display()
